<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="eventPageLoaded">
  <DockLayout stretchLastChild="true">
    <ListView dock="top" items="{{events}}" itemTap="expandDetail">
	    <ListView.itemTemplate>
        <Label horizontalAlignment="stretch"
              height="50px" text="{{EventDate + ': ' + EventName}}" />

	    </ListView.itemTemplate>
    </ListView>
  </DockLayout>
</Page>
